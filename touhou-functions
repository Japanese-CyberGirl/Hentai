import numpy as np
def enterMatrix(x,y):
    """
    заполение массива расстояний на основе координат
    :return:
    """
    counter = 0
    branch = np.zeros([(x), (y)])  # Шаблон матрицы
    for i in np.arange(0, x, 1):
        for j in np.arange(0, y, 1):
                branch[i, j] = float('inf')
                counter += 1
    return branch
